<?xml version="1.0" encoding="utf-8"?>
<ca.cours5b5.williamsarrazin.vues.VMenuPrincipal xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="ca.cours5b5.williamsarrazin.activites.AMenuPrincipal">

    <Button
        android:id="@+id/bouton_parametres"
        android:layout_width="116dp"
        android:layout_height="38dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/parametres"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bouton_partie"
        tools:layout_editor_absoluteY="338dp" />

    <Button
        android:id="@+id/bouton_partie"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/jouer"
        app:layout_constraintBottom_toTopOf="@+id/bouton_parametres"
        app:layout_constraintEnd_toEndOf="@+id/bouton_parametres"
        app:layout_constraintStart_toStartOf="@+id/bouton_parametres"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/bouton_jouer_en_ligne"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/jouer_en_ligne"
        app:layout_constraintBottom_toTopOf="@+id/bouton_partie"
        app:layout_constraintEnd_toEndOf="@+id/bouton_partie"
        app:layout_constraintStart_toStartOf="@+id/bouton_partie"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnConnexion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/connexion"
        app:layout_constraintBottom_toTopOf="@+id/bouton_parametres"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bouton_partie" />
</ca.cours5b5.williamsarrazin.vues.VMenuPrincipal>
